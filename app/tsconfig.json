{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": false,
      "removeComments": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "out",
      "types": []
    },
    "include": [
      "src/**/*.ts", "JiraLinkApp.ts", "test/**/*.ts"
    ],
    "exclude": [
        "node_modules/**/*"
    ]
  }

